{
  "name": "nomos",
  "version": "0.1.0",
  "description": "TypeScript CLI AI agent powered by Anthropic models",
  "keywords": [
    "agent",
    "ai",
    "anthropic",
    "chatbot",
    "claude",
    "cli",
    "nomos",
    "typescript"
  ],
  "homepage": "https://github.com/user/nomos#readme",
  "bugs": {
    "url": "https://github.com/user/nomos/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/nomos.git"
  },
  "bin": {
    "nomos": "dist/index.js"
  },
  "files": [
    "dist",
    "skills",
    "src/db/schema.sql"
  ],
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "playwright install chromium",
    "build": "tsdown",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "check": "pnpm format:check && pnpm typecheck && pnpm lint",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix && pnpm format",
    "format": "oxfmt --write",
    "format:check": "oxfmt --check",
    "test": "vitest run",
    "test:watch": "vitest",
    "daemon:dev": "tsx src/daemon/index.ts",
    "daemon:start": "node dist/index.js daemon start",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.45",
    "@hapi/boom": "^10.0.1",
    "@slack/bolt": "^4.6.0",
    "@slack/web-api": "^7.14.1",
    "@whiskeysockets/baileys": "7.0.0-rc.9",
    "better-sqlite3": "^12.6.2",
    "chalk": "^5.4.0",
    "chokidar": "^5.0.0",
    "cli-highlight": "^2.1.11",
    "commander": "^14.0.3",
    "cron-parser": "^5.5.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1",
    "google-auth-library": "^10.5.0",
    "grammy": "^1.40.0",
    "ink": "^6.7.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "playwright": "^1.50.0",
    "postgres": "^3.4.7",
    "react": "^19.2.4",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.12.0",
    "@types/react": "^19.2.14",
    "@types/ws": "^8.18.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "0.33.0",
    "oxlint": "^1.48.0",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "oxfmt --write",
      "oxlint"
    ],
    "*.{md,json}": [
      "oxfmt --write"
    ]
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.23.0"
}
